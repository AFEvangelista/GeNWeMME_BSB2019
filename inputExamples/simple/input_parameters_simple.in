# TYPE 1 inputs:
#   MAF_FILE_NAME
#   GENE_NETWORK_FILE_NAME

# TYPE 2 inputs:
#   BMM_FILE_NAME
#   WMM_FILE_NAME
#   RGN_FILE_NAME

# TYPE 3 inputs:
#   MAF_FILE_NAME
#   WEIGHTED_GENE_NETWORK_FILE_NAME

# TYPE 4 inputs:
#   BMM_FILE_NAME
#   WMM_FILE_NAME
#   WEIGHTED_GENE_NETWORK_FILE_NAME


[INPUT_TYPE]
TYPE: 1

[INPUT]
MAF_FILE_NAME: inputExamples/simple/simple.maf
BMM_FILE_NAME: inputExamples/simple/simple.bmm
WMM_FILE_NAME: inputExamples/simple/simple.wmm
GENE_NETWORK_FILE_NAME: inputExamples/simple/geneNetwork_3_simple_maf.txt
WEIGHTED_GENE_NETWORK_FILE_NAME: inputExamples/simple/geneNetwork_3_simple_weighted.txt
RGN_FILE_NAME: inputExamples/simple/simple.rgn
PRE_PROCESSING_THRESHOLD_PHI: 0.02
RGN_THRESHOLD_GAMMA: 0.6
COMPONENT_SIZE_K: 3
MUTATION_RELEVANCE_ALPHA: 1
RELATED_GENES_BETA: 1
MUTUAL_EXCLUSIVITY_DELTA: 1

[VARIANT_CLASSIFICATION_WEIGHTS]
NONSENSE: 1.0
MISSENSE: 0.4
SPLICE: 0.4
FRAMESHIFT_DEL: 1.0
FRAMESHIFT_INS: 1.0
INFRAME_DEL: 0.4
INFRAME_INS: 0.4
_3UTR: 0.2
_5UTR: 0.4
NONSTOP_MUTATION: 0.4
TRANSLATION_START_SITE: 0.2

[OUTPUT]
OUTPUT_FOLDER: outputExamples/output_simple
WMM_FILE: simple.wmm
BMM_FILE: simple.bmm
AM_FILE: simple.am
RGN_FILE: simple.rgn
COMPONENTS_FILE: simple.components
RANKING_FILE: simple.ranking
SAMPLE_REPORT_FILE: simple.sample.report
GENE_REPORT_FILE: simple.gene.report
NETWORK_REPORT_FILE: simple.network.report
